<window title="Probot Control" width="max" height="max" theme="Dark">
    <!--    <widget title="ex2" type="video" draggable="True" x="0" y="0" id="2"/>-->
    <widget type="tab" title="Dashboard" height="max" width="max" x="0" y="0" static="True" background="theme">
        <tab title="Tab1">
            <widget title="test button" type="simpleButton" x="100" y="200" id="simpleButtonOut"/>
            <widget title="Destroy the world" type="simpleButton" x="100" y="300" id="button2"/>
            <widget title="Evaporate mars" type="simpleButton" x="100" y="400" id="button3"/>
            <widget title="TestVideo" type="video" draggable="True" x="300" y="0" id="1" height="200" width="400"/>
            <widget title="Power Stats" type="textBox" x="0" y="0" draggable="True" background="none" textColor="red">
                <line label="Thing 1" value="KEY1"/>
                <line label="Thing 2" value="KEY2"/>
                <line label="Thing 3" value="KEY3"/>
                <line label="Thing 4" value="KEY4"/>
            </widget>
            <widget title="TestGraph" type="multiBarGraph" draggable="True" x="300" y="300">
                <line type="SimpleBarGraph" source="roll" title="A" minimum="-20.0" maximum="20.0" color="rgb(0,0,255)"/>
                <line type="CircleBarGraph" source="j" title="B" minimum="0.0" maximum="360.0" color="rgb(255,0,0)"/>
                <line type="SimpleBarGraph" source="verticalSpeed" title="Graph C" minimum="0.0" maximum="20.0" color="rgb(255,0,158)"/>
                <line type="SimpleBarGraph" source="terrainAlt" title="D" minimum="0.0" maximum="100.0" color="rgb(60,255,40)"/>
                <line type="SimpleBarGraph" source="j" title="Title" minimum="0.0" maximum="360.0" color="rgb(255,255,255)"/>
            </widget>
            <widget type="aa" x="300 y=500"/>
        </tab>
        <tab title="Tab2">
            <widget title="ex2" type="video" height="480" width="640" draggable="True" x="0" y="0" id="2"/>
        </tab>
        <tab title="Tab3">
            <widget title="Power Stats" type="textBox" x="100" y="300" draggable="True" background="green">
                <line label="Thing 1" value="KEY1"/>
                <line label="Thing 2" value="KEY2"/>
                <line label="Thing 3" value="KEY3"/>
                <line label="Thing 4" value="KEY4"/>
            </widget>
            <widget type="tab" title="Dashboard" width="500" height="500" x="50" y="50" draggable="False" static="False">
                <tab title="Tab1">
                    <widget title="TestVideo" type="video" height="150" width="200" draggable="True" x="100" y="0" id="1"/>
                    <widget title="Power Stats" type="textBox" x="100" y="300" draggable="True" background="green">
                        <line label="Thing 1" value="KEY1"/>
                        <line label="Thing 2" value="KEY2"/>
                        <line label="Thing 3" value="KEY3"/>
                        <line label="Thing 4" value="KEY4"/>
                    </widget>
                </tab>
                <tab title="Tab2">
                    <widget title="ex2" type="video" height="200" width="250" draggable="True" x="0" y="0" id="2"/>
                </tab>
                <tab title="Tab3">

                </tab>
            </widget>
        </tab>
    </widget>
</window>